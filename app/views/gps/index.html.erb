<p id="notice"><%= notice %></p>

<h1>All GPS Waypoints</h1>

<table>
  <thead>
    <tr>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Vehicle Identifier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gps.each do |gp| %>
      <tr>
        <td><%= gp.longitude %></td>
        <td><%= gp.latitude %></td>
        <td><%= gp.vehicle.vehicle_identifier  %></td>
        <td><%= link_to 'Show', gp %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Position', new_gp_path %>
